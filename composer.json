{
    "name": "aaronschmied/laravel-smtpd",
    "description": "An smtp server for recieving emails with laravel.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Aaron Schmied",
            "email": "aaron@pro-sales.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "Smtpd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smtpd\\Providers\\SmtpdServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.1",
        "ext-sockets": ">=0",
        "zendframework/zend-mail": "^2.3",
        "symfony/options-resolver": "^3.3",
        "psr/log": "^1.0",
        "illuminate/mail": "^5.8",
        "illuminate/log": "^5.8",
        "illuminate/queue": "^5.8",
        "illuminate/config": "^5.8",
        "illuminate/events": "^5.8",
        "illuminate/console": "^5.8",
        "illuminate/auth": "^5.8",
        "illuminate/support": "^5.8"
    }
}
